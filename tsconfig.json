{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@consts/*": ["./src/consts/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@pages-components/*": ["./src/components/pages-components/*"],
      "@utils/*": ["./src/utils/*"],
    }
  }
}
